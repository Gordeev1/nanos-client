{
	"name": "NanoAds",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"start": "react-native start",
		"test": "jest",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npx tsc --noEmit && lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,ts,tsx}": [
			"eslint --fix",
			"git add"
		]
	},
	"checkmate": {
		"verbose": false,
		"silentShell": true,
		"programs": [
			"node",
			"npm",
			"react-native",
			"watchman",
			"xcodebuild"
		]
	},
	"dependencies": {
		"@react-native-community/masked-view": "^0.1.7",
		"@react-navigation/native": "^5.0.10",
		"@react-navigation/stack": "^5.2.0",
		"@reduxjs/toolkit": "^1.2.5",
		"axios": "^0.21.1",
		"i18n-js": "^3.5.1",
		"lodash": "^4.17.15",
		"react": "16.9.0",
		"react-native": "0.61.5",
		"react-native-config": "^1.0.0",
		"react-native-fast-image": "^8.1.5",
		"react-native-gesture-handler": "^1.6.0",
		"react-native-iphone-x-helper": "^1.2.1",
		"react-native-localize": "^1.3.4",
		"react-native-reanimated": "^1.7.0",
		"react-native-safe-area-context": "^0.7.3",
		"react-native-screens": "^2.3.0",
		"react-native-snap-carousel": "^3.8.4",
		"react-native-svg": "^12.0.3",
		"react-redux": "^7.2.0",
		"redux": "^4.0.5",
		"styled-components": "4.4.0",
		"victory-native": "^34.1.0"
	},
	"devDependencies": {
		"@babel/core": "^7.6.2",
		"@babel/runtime": "^7.6.2",
		"@react-native-community/eslint-config": "^0.0.5",
		"@types/i18n-js": "^3.0.1",
		"@types/jest": "^24.0.24",
		"@types/lodash": "^4.14.149",
		"@types/react": "^16.9.23",
		"@types/react-native": "^0.60.25",
		"@types/react-native-snap-carousel": "^3.8.1",
		"@types/react-redux": "^7.1.7",
		"@types/react-test-renderer": "16.9.1",
		"@types/styled-components": "4.4.0",
		"@typescript-eslint/eslint-plugin": "^2.12.0",
		"@typescript-eslint/parser": "^2.12.0",
		"babel-jest": "^24.9.0",
		"eslint": "^6.5.1",
		"husky": "^4.2.3",
		"jest": "^24.9.0",
		"lint-staged": "^10.0.8",
		"metro-react-native-babel-preset": "^0.56.0",
		"prettier": "^1.19.1",
		"react-native-checkmate": "^0.2.0",
		"react-test-renderer": "16.9.0",
		"typescript": "^3.7.3"
	},
	"jest": {
		"preset": "react-native",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	}
}
